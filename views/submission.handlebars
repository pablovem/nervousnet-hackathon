<div class="container">
  <div class="page-header">
    <h1>Submission</h1>
  </div>

  <div class="row">
    <div class="col-md-12">
      <form role="form" id="submForm" action="/api/submission" method="post" enctype="multipart/form-data">
        <label class="control-label">Select your .zip file:</label>
        <input id="input-2" name="submission" type="file" class="file" multiple data-show-upload="false" data-show-caption="true">
        <hr>
        <button type="submit" class="btn btn-primary btn-lg btn-block btn-nn">Submit</button>
      </form>
    </div>
  </div>
</div>

